-- CRIANDO A TABELA 
CREATE TABLE pessoas (
    nome VARCHAR(20),
    nascimento DATE
)

-- INSERINDO DADOS NA TABELA
INSERT INTO pessoa (nome, nascimento) VALUES ('Nathally', '1990-05-22');
